# Adding paths one by one
path_to_add=(
    "/Users/martin/Scripts"
    "/opt/homebrew/opt/mysql-client/bin"
)

for p in $path_to_add; do
    if [[ -d $p && ! $PATH =~ $p ]]; then
        export PATH="$PATH:$p"
    fi
done



